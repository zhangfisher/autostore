    "autostore": "workspace:*",
    "flexstyled": "^2.1.2"
  },
  "devDependencies": {
    "@rsbuild/plugin-react": "^1.0.7",
    "@rslib/core": "^0.0.18",
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.3",
    "react": "^18.3.1",
    "rsbuild-plugin-dts": "^0.0.18",
    "terser": "^5.36.0",
    "tsup": "^8.5.0",
    "type-fest": "^4.26.1",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "react": "^18.3.1"
