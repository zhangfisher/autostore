		"flex-tools": "^1.5.9",
		"lit": "3.2.1",
		"themepro": "^1.0.7"
	},
	"devDependencies": {
		"autostore": "workspace:*",
		"esbuild-copy-files-plugin": "^1.2.1",
		"tsup": "^8.5.0",
		"typescript": "~5.9.3",
		"vite": "^7.1.9",
		"vite-plugin-cp": "^6.0.3",
		"vite-plugin-mock-dev-server": "^2.0.1",
