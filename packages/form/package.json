{
    "name": "@autostorejs/form",
    "version": "3.2.11",
    "description": "form webcomponent for autostore",
    "type": "module",
    "exports": {
        ".": "./dist/index.js",
        "./browser": "./dist/index.global.js",
        "./styles": "./dist/index.css"
    },
    "publishConfig": {
        "access": "public",
        "main": "dist/index.js",
        "exports": {
            ".": {
                "import": "./dist/index.js",
                "require": "./dist/index.cjs"
            },
            "./browser": "./dist/index.global.js",
            "./styles": "./dist/index.css"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "vite",
        "build": "tsup",
        "build:vite": "tsc && vite build",
        "preview": "vite preview"
    },
    "dependencies": {
        "@iconify-json/lucide": "^1.2.86",
        "@lit/context": "^1.1.6",
        "@shoelace-style/shoelace": "^2.20.1",
        "flex-tools": "^1.5.9",
        "lit": "3.3.2",
        "themepro": "^1.0.7"
    },
    "devDependencies": {
        "autostore": "workspace:*",
        "@autostorejs/syncer": "workspace:*",
        "esbuild-copy-files-plugin": "^1.2.1",
        "tsup": "^8.5.0",
        "typescript": "~5.9.3",
        "vite": "^7.1.9",
        "vite-plugin-cp": "^6.0.3",
        "vite-plugin-mock-dev-server": "^2.0.1",
        "zlib": "^1.0.5"
    },
    "peerDependencies": {
        "autostore": "workspace:*"
    }
}
