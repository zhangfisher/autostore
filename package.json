{
	"name": "autostore-workspace",
	"version": "1.0.0",
	"description": "autostore",
	"main": "index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/zhangfisher/autostore.git"
	},
	"scripts": {
		"docs:dev": "vitepress dev docs",
		"docs:build": "vitepress build docs",
		"changeset": "changeset",
		"publish:all": "turbo build  && changeset version && changeset publish",
		"build": "turbo build --filter=autostore --filter=@autostorejs/syncer --filter=@autostorejs/devtools --filter=@autostorejs/react",
		"docs:preview": "vitepress preview new-docs",
		"sync": "cnpm sync autostore @autostorejs/react @autostorejs/form @autostorejs/syncer",
		"update:changelog": "cpx packages/core/CHANGELOG.md docs/zh/guide/intro/core.changelog.md",
		"lint": "oxlint",
		"lint:fix": "oxlint --fix"
	},
	"keywords": [],
	"author": "zhangfisher",
	"license": "MIT",
	"devDependencies": {
		"@changesets/cli": "^2.29.8",
		"@types/node": "^25.0.6",
		"oxfmt": "^0.27.0",
		"oxlint": "^1.42.0",
		"oxlint-tsgolint": "^0.11.2",
		"turbo": "^2.7.5",
		"typescript": "^5.9.3",
		"vitepress": "^1.6.4",
		"vue": "^3.5.27"
	},
	"workspaces": [
		"packages/*",
		"examples/*",
		"docs"
	],
	"packageManager": "bun@1.3.6"
}
