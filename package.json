{
	"name": "autostore-workspace",
	"version": "1.0.0",
	"description": "autostore",
	"main": "index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/zhangfisher/autostore.git"
	},
	"scripts": {
		"docs:dev": "vitepress dev docs",
		"docs:build": "vitepress build docs",
		"changeset": "changeset",
		"publish:all": "turbo build  && changeset version && changeset publish",
		"build": "turbo build",
		"docs:preview": "vitepress preview new-docs",
		"sync": "cnpm sync autostore @autostorejs/react @autostorejs/form @autostorejs/syncer",
		"update:changelog": "cpx packages/core/CHANGELOG.md docs/zh/guide/intro/core.changelog.md"
	},
	"keywords": [],
	"author": "zhangfisher",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "2.3.11",
		"@changesets/changelog-github": "^0.5.0",
		"@changesets/cli": "^2.29.6",
		"@types/node": "^25.0.3",
		"turbo": "^2.5.6",
		"typescript": "^5.9.3",
		"vitepress": "^1.6.4",
		"vue": "^3.5.14"
	},
	"workspaces": [
		"packages/*",
		"examples/*",
		"docs"
	],
	"packageManager": "bun@1.3.5"
}
